{
    "license": "MIT",
    "name": "structure_base",
    "version": "1.0.0",
    "main": "./src/index.ts",
    "type": "module",
    "scripts": {
        "ts-node": "ts-node",
        "start:dev": "nodemon",
        "start:debug": "npm run start --debug --watch",
        "build": "rimraf ./build && tsc",
        "start": "npm run build && node build/index.js",
        "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
        "generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n migration -p",
        "generate-dry": "ts-node ./node_modules/typeorm/cli.js migration:generate -n migration -p --dr",
        "make-migration": "npx typeorm migration:create -n migrationName"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.5",
        "@babel/preset-env": "^7.21.5",
        "@types/bcrypt": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/koa": "^2.13.5",
        "@types/koa__router": "^12.0.0",
        "@types/koa-bodyparser": "^4.3.10",
        "@types/node": "^18.16.6",
        "nodemon": "^2.0.20",
        "rimraf": "^4.1.2",
        "ts-node": "^10.9.1",
        "tsc": "^2.0.4",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@koa/router": "^12.0.0",
        "bcrypt": "^5.1.0",
        "class-transformer": "^0.5.1",
        "dotenv": "^16.0.3",
        "jsonwebtoken": "^9.0.0",
        "koa": "^2.14.1",
        "koa-bodyparser": "^4.4.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.15"
    }
}
